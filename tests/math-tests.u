
use Math minus sumsq zeros
use Sequence foldb replicate zipWith
use Test.Result Ok Fail
use Text ++

-- Sequence
test> if ((replicate 4 0) == [0, 0, 0, 0]) then Ok "Sequence.replicate"
  else Fail "(replicate 4 0) != [0, 0, 0, 0]"
test> if ((zipWith (++) ["a"] ["b"]) == ["ab"]) then Ok "Sequence.zipWith"
  else Fail "(zipWith (++) [a] [b]) != [ab]"

-- Math
test> if ((zeros 4) == [ +0, +0, +0, +0 ]) then Ok "Math.zeros"
  else Fail "(zeros 4) != [ +0, +0, +0, +0 ]"

x = [ +1, +4, +9 ]

test> if ((sumsq x x) == [ +1, +16, +81 ]) then Ok "Math.sumsq"
  else Fail "(sumsq x x) != [ +1, +16, +81 ]"
test> if ((minus x x) == [ +0, +0, +0 ]) then Ok "Math.minus"
  else Fail "(minus x x) != [ +0, +0, +0 ]"
